<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/Boton1"
        android:layout_width="177dp"
        android:layout_height="124dp"
        android:layout_gravity="center"
        android:layout_marginStart="59dp"
        android:layout_marginTop="94dp"
        android:layout_marginEnd="228dp"
        android:layout_marginBottom="281dp"
        android:clickable="true"
        android:gravity="center"
        android:onClick="clickarBoton"
        android:scaleType="center"
        app:layout_constraintBottom_toTopOf="@+id/Boton3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:fabCustomSize="123dp"
        app:maxImageSize="110dp"
        app:srcCompat="@drawable/los40"
        app:tint="@null" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/Boton2"
        android:layout_width="177dp"
        android:layout_height="124dp"
        android:layout_marginStart="236dp"
        android:layout_marginTop="280dp"
        android:layout_marginEnd="51dp"
        android:layout_marginBottom="327dp"
        android:clickable="true"
        android:onClick="clickarBoton"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:fabCustomSize="123dp"
        app:maxImageSize="110dp"
        app:srcCompat="@drawable/los40classic"
        app:tint="@null"/>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/Boton3"
        android:layout_width="177dp"
        android:layout_height="124dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="281dp"
        android:layout_marginEnd="228dp"
        android:clickable="true"
        android:onClick="clickarBoton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Boton1"
        app:fabCustomSize="123dp"
        app:maxImageSize="110dp"
        app:srcCompat="@drawable/los40dance"
        app:tint="@null"/>

</androidx.constraintlayout.widget.ConstraintLayout>